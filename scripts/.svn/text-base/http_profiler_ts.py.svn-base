import sys
import os
sys.path.append( os.path.join( os.getcwd(), '..' ) )
import wapf


baseUrl = 'http://www.zumiez.com/'
browser = 'firefox'
enableWrite = 0

#create class instance
tobj = wapf.wapf(baseUrl, browser) #init baseUrl and browser

#Get all body links in selected url
linksList = tobj.getAllUniqueLinks(baseUrl)

count = 1

#loop them out to log file (if enabled)
for link in linksList:
	message = tobj.setHeaderMessage('Web Profiling: (' + str(count) + ')' , enableWrite)
	profiler = tobj.runProfiler(link, '/', browser, enableWrite)
	count +=1

